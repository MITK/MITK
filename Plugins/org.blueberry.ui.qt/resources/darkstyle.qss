QTabWidget::pane {
  border: 1px solid #434346;
}

QTabBar::tab {
  background-color: #434346;
  border: 1px solid #434346;
  border-bottom: none;
  padding: 4px;
}

QTabBar::tab:middle {
  border-left: none;
}

QTabBar::tab:last {
