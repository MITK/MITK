}

QTabBar::tab:selected {
  border: 1px solid #434346;
  border-bottom: none;
}

QTabBar::tab:!selected {
  background-color: #2d2d30;
}

QTabBar::tab:!selected:hover {
  background-color: #434346;
}

#TabCloseButton {
